@import '../../stylus/index.styl'

.receivedCountMessage
  background-color: $color2
  color: #fff
  padding: $spacing*2 0
  .all
    font-size: 18px
    line-height: 1
    padding: 0 $spacing $spacing
  .descs
    font-size: 13px
    line-height: 1.25
    display: flex
    li
      box-sizing: border-box
      flex: 1
      padding-left: $spacing
      &:not(:nth-last-child(1)){
        border-right: 1px solid $borderColor
      }
