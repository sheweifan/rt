@import './stylus/index'

body
  margin: 0
  padding: 0
  font-family: sans-serif

ul, p
  margin: 0
  padding: 0

img
  width: 100%
  display: block

li
  list-style: none

em
  font-style: normal

.fullBtn
  background: $color
  color: #ffffff
  text-align: center
  height: 47px
  line-height: 47px
  font-size: 18px
  &.disabled
    background: #999

.am-button-primary,
.am-button-active
  background-color: $color !important
  border-color: $color !important

.am-button.am-button-primary:before
  border-color: $color !important
